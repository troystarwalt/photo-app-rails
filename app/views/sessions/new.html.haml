.off-canvas-content{"data-off-canvas-content" => ""}
  .title-bar.hide-for-large
    .title-bar-left
      %button.menu-icon{"data-open" => "my-info", :type => "button"}
      %span.title-bar-title This will change depending on page.
  .welcome-msg
    .row
      .column
        %h1 Login
  .main-content-container
    .row
      .column
        = form_for(:session, url: login_path) do |f|
          = f.label :email
          = f.email_field :email
          = f.label :password
          = f.password_field :password
          = f.submit "Login"
