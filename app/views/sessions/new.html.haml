.welcome-msg
  .row
    .column
      = render "partials/flashes"
      %h1 Login
.main-content-container
  .row
    .columns.medium-6
      = form_for(:session, url: login_path) do |f|
        = f.label :email
        = f.email_field :email
        = f.label :password
        = f.password_field :password
        = f.submit "Login", class: "button"
